{
  "name": "async-basics",
  "version": "1.0.0",
  "description": "How to deal with asynchronous JavaScript",
  "scripts": {
    "clean": "rimraf build",
    "build": "babel src --source-maps -d build",
    "start": "DEBUG=callback,promise,async node --require source-map-support/register build/app.js",
    "watch": "nodemon --watch src --exec 'npm-run-all -s build start'"
  },
  "keywords": [
    "async",
    "callback",
    "promise"
  ],
  "author": "Jérôme Ludmann",
  "license": "ISC",
  "dependencies": {
    "debug": "^2.2.0",
    "npm-run-all": "^3.0.0",
    "source-map-support": "^0.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "nodemon": "^1.10.0",
    "rimraf": "^2.5.4"
  }
}
